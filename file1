$url='https://github.com/TotallyNotThat/testRep/raw/main/MemoryTest.zip';
$file_name = basename($url);
if(file_put_contents($file_name, file_get_contents($url))){
    $zip = new ZipArchive;
    $res = $zip->open('MemoryTest.zip');
    if ($res === TRUE) {
    $zip->extractTo(getcwd());
    $zip->close();
    echo 'woot!';
    } else {
    echo 'doh!';
    }
}